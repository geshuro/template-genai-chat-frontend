{
  "name": "metatron-ia-frontend",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --aot --delete-output-path",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "sonar": "./node_modules/sonar-scanner/bin/sonar-scanner",
    "lint": "ng lint --fix",
    "prettier": "npx prettier --write 'src/**/*.{ts,html,scss,json}'",
    "prepare": "husky",
    "pre-commit": "npm run lint && npm run prettier",
    "format": "prettier . --write --ignore-unknown"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.2.2",
    "@angular/common": "^17.2.2",
    "@angular/compiler": "^17.2.2",
    "@angular/core": "^17.2.2",
    "@angular/forms": "^17.2.2",
    "@angular/material": "^17.3.7",
    "@angular/platform-browser": "^17.2.2",
    "@angular/platform-browser-dynamic": "^17.2.2",
    "@angular/router": "^17.2.2",
    "@angular/service-worker": "^17.2.2",
    "@azure/msal-angular": "^3.0.20",
    "@azure/msal-browser": "^3.17.0",
    "@ctrl/ngx-emoji-mart": "^9.2.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "dexie": "^4.0.4",
    "driver.js": "^1.3.1",
    "js-sha256": "^0.11.0",
    "jwt-decode": "^4.0.0",
    "marked": "^5.1.0",
    "ngx-google-analytics": "^14.0.1",
    "ngx-skeleton-loader": "^9.0.0",
    "pdfmake": "^0.2.10",
    "rxjs": "~7.8.1",
    "sonar-scanner": "^3.1.0",
    "source-map-support": "^0.5.21",
    "tslib": "^2.6.2",
    "uuid": "^9.0.1",
    "zone.js": "~0.14.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.2.1",
    "@angular-eslint/builder": "^17.4.0",
    "@angular-eslint/eslint-plugin": "^17.4.0",
    "@angular-eslint/eslint-plugin-template": "^17.4.0",
    "@angular-eslint/schematics": "^17.4.0",
    "@angular-eslint/template-parser": "^17.4.0",
    "@angular/cli": "^17.2.2",
    "@angular/compiler-cli": "^17.2.2",
    "@types/jasmine": "~5.1.4",
    "@types/marked": "^5.0.2",
    "@types/pdfmake": "^0.2.9",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "@typescript-eslint/parser": "7.8.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jasmine-core": "~5.1.2",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "typescript": "~5.2"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
